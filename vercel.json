{
  "routes": [
    {
      "src": "/assets/(.*).js",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "dest": "/dist/assets/$1.js"
    },
    {
      "src": "/assets/(.*).css",
      "headers": {
        "Content-Type": "text/css"
      },
      "dest": "/dist/assets/$1.css"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
